{"id":{{post.id}}, "html":"{% load static %}<div class='well post-item' id='{{ post.id }}' ><span>{% if post.user.profile.picture.name %}<img class='crop' src={% url 'photo' post.user.username %} id='profile_picture' >{% else %}<img src={% static 'images/img.jpg' %} id='profile_picture'>{% endif %}</span><span><span><a href={% url 'profile' post.user.username %}>{{ post.user.first_name }}</a></span><span class='time'>{{ post.last_changed |date:"Y-m-d H:i" }}</span><div class='post_content'>{{ post.content }}</div></span><div class='row row-offcanvas comment-line'><div class='col-xs-11 col-sm-11 blog-main'><input id='comment-field{{ post.id }}' type='text' class='form-control' placeholder='Leave a comment...' name='comment' maxlength='420'></div><div class='col-xs-1 col-sm-1 blog-main'><button name='comment-button{{ post.id }}' class='btn btn-primary comment-button' onclick='addComment({{ post.id }})'>Comment222</button></div></div><div id='comment-list{{ post.id }}' class='comment-list'></div>{% ifequal username post.user.username %}<form method='post' action={% url 'delete' post.id %} ><span> <input type='submit' class='btn btn-xs btn-danger delete-btn' value='Delete'></span>{% endifequal %}{% csrf_token %}</form></div>"}

