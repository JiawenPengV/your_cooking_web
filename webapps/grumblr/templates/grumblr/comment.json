{"html":"{% load static %}<div class='well comment-block'><div class='comment-header'><a  href={% url 'profile' comment.user.username %}>{% if comment.user.profile.picture.name %}<img class='crop' src={% url 'photo' comment.user.username %} id='profile_picture'>{% else %}<img src={% static 'images/img.jpg' %} id='profile_picture'>{% endif %}<span>{{ comment.user.first_name }}</span></a><span class='time'>{{ comment.last_changed |date:"Y-m-d H:i" }}</span></div><div class='comment_content'>{{ comment.content }}</div></div>"}
